    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'starter-snake-rust'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=snake",
                    "--package=snake"
                ],
                "filter": {
                    "name": "snake",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}",
            "env": {
                "RUST_LOG": "warning,snake=trace",
                "ROCKET_PORT": "8001"
            }
        }
    ]
}